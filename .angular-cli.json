{
  "$schema": "./node_modules/@nrwl/schematics/src/schema.json",
  "project": {
    "name": "ngair-platform",
    "npmScope": "ngair-platform",
    "latestMigration": "20180313-add-tags"
  },
  "e2e": {
    "protractor": {
      "config": "./protractor.conf.js"
    }
  },
  "lint": [
    {
      "project": "./tsconfig.spec.json",
      "exclude": "**/node_modules/**"
    },
    {
      "project": "apps/website/src/tsconfig.app.json",
      "exclude": "**/node_modules/**"
    },
    {
      "project": "apps/website/e2e/tsconfig.e2e.json",
      "exclude": "**/node_modules/**"
    },
    {
      "project": "apps/website-prerender/src/tsconfig.app.json",
      "exclude": "**/node_modules/**"
    },
    {
      "project": "apps/website-prerender/e2e/tsconfig.e2e.json",
      "exclude": "**/node_modules/**"
    }
  ],
  "test": {
    "karma": {
      "config": "./karma.conf.js"
    }
  },
  "defaults": {
    "schematics": {
      "collection": "@nrwl/schematics",
      "postGenerate": "npm run format",
      "newProject": [
        "app",
        "lib"
      ]
    },
    "styleExt": "css",
    "component": {}
  },
  "warnings": {
    "typescriptMismatch": false
  },
  "apps": [
    {
      "name": "website",
      "root": "apps/website/src",
      "outDir": "dist/apps/website",
      "assets": [
        "assets",
        "favicon.ico",
        {
          "glob": "ngair-logo.png",
          "input": "../../../libs/website/assets/src/",
          "output": "./images/"
        }
      ],
      "index": "index.html",
      "main": "main.ts",
      "polyfills": "polyfills.ts",
      "test": "../../../test.js",
      "tsconfig": "tsconfig.app.json",
      "testTsconfig": "../../../tsconfig.spec.json",
      "prefix": "app",
      "styles": [
        "styles.scss"
      ],
      "scripts": [],
      "environmentSource": "environments/environment.ts",
      "environments": {
        "dev": "environments/environment.ts",
        "prod": "environments/environment.prod.ts"
      },
      "tags": []
    },
    {
      "name": "website-prerender",
      "root": "apps/website-prerender/src",
      "outDir": "dist/apps/website-prerender",
      "assets": [
        "assets",
        "favicon.ico"
      ],
      "index": "index.html",
      "main": "main.ts",
      "polyfills": "polyfills.ts",
      "test": "../../../test.js",
      "tsconfig": "tsconfig.app.json",
      "testTsconfig": "../../../tsconfig.spec.json",
      "prefix": "app",
      "styles": [
        "styles.css"
      ],
      "scripts": [],
      "environmentSource": "environments/environment.ts",
      "environments": {
        "dev": "environments/environment.ts",
        "prod": "environments/environment.prod.ts"
      },
      "tags": []
    },
    {
      "name": "website/app",
      "root": "libs/website/app/src",
      "test": "../../../../test.js",
      "appRoot": "",
      "tags": []
    },
    {
      "name": "website/common-ui",
      "root": "libs/website/common-ui/src",
      "test": "../../../../test.js",
      "appRoot": "",
      "tags": []
    },
    {
      "name": "website/page-episode",
      "root": "libs/website/page-episode/src",
      "test": "../../../../test.js",
      "appRoot": "",
      "tags": []
    },
    {
      "name": "website/page-home",
      "root": "libs/website/page-home/src",
      "test": "../../../../test.js",
      "appRoot": "",
      "tags": []
    }
  ]
}